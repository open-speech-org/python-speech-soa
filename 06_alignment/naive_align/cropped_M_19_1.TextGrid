            intervals [217]:
                xmin = 505.06
                xmax = 506.32
                text = " Y se entusiasmaron tanto"
            intervals [218]:
                xmin = 506.87
                xmax = 507.27
                text = " que al rato la jaula entera temblaba con las sacudidas y hacía un terrible ruido"
            intervals [219]:
                xmin = 508.25
                xmax = 508.41
                text = " Tal ruido hacía"
            intervals [220]:
                xmin = 508.95
                xmax = 508.97
                text = " que el perro se despertó"
            intervals [221]:
                xmin = 510.59
                xmax = 510.66
                text = " lanzando un ronco ladrido"
            intervals [222]:
                xmin = 511.33
                xmax = 511.42
                text = " Mas los coatís no esperaron a que el perro les pidiera cuenta de ese escándalo y dispararon al monte"
            intervals [223]:
                xmin = 512.04
                xmax = 512.43
                text = " dejando la lima tirada"
            intervals [224]:
                xmin = 514.74
                xmax = 515.68
                text = "          Al día siguiente"
            intervals [225]:
                xmin = 516.2
                xmax = 516.62
                text = " los chicos fueron temprano a ver a su nuevo huésped"
            intervals [226]:
                xmin = 519.33
                xmax = 519.98
                text = " que estaba muy triste"
            intervals [227]:
                xmin = 522.11
                xmax = 522.69
                text = "Qué nombre le pondremos"
            intervals [228]:
                xmin = 523.59
                xmax = 524.22
                text = "preguntó la nena a su hermano"
            intervals [229]:
                xmin = 527.53
                xmax = 527.57
                text = "Ya sé"
            intervals [230]:
                xmin = 530.29
                xmax = 531.13
                text = "respondió el varoncito"
            intervals [231]:
                xmin = 531.83
                xmax = 531.85
                text = "Le pondremos Diecisiete"
            intervals [232]:
                xmin = 533.19
                xmax = 533.23
                text = "Por qué Diecisiete"
            intervals [233]:
                xmin = 538.04
                xmax = 539.61
                text = " Nunca hubo bicho del monte con nombre más raro"
            intervals [234]:
                xmin = 541.01
                xmax = 542.17
                text = " Pero el varoncito estaba aprendiendo a contar"
            intervals [235]:
                xmin = 544.0
                xmax = 544.59
                text = " y tal vez le había llamado la atención aquel número"
            intervals [236]:
                xmin = 546.8
                xmax = 546.84
                text = "          El caso es que se llamó Diecisiete"
            intervals [237]:
                xmin = 548.89
                xmax = 549.43
                text = " Le dieron pan"
            intervals [238]:
                xmin = 550.18
                xmax = 550.2
                text = " uvas"
            intervals [239]:
                xmin = 550.85
                xmax = 550.9
                text = " chocolate"
            intervals [240]:
                xmin = 552.87
                xmax = 553.14
                text = " carne"
            intervals [241]:
                xmin = 555.8
                xmax = 556.42
                text = " langostas"
            intervals [242]:
                xmin = 557.25
                xmax = 557.3
                text = " huevos"
            intervals [243]:
                xmin = 557.99
                xmax = 558.01
                text = " riquísimos huevos de gallina"
            intervals [244]:
                xmin = 559.24
                xmax = 559.25
                text = " lograron que en un solo día se dejara rascar la cabeza"
            intervals [245]:
                xmin = 560.8
                xmax = 561.44
                text = " y tan grande es la sinceridad del cariño de las criaturas"
            intervals [246]:
                xmin = 563.94
                xmax = 564.25
                text = " que"
            intervals [247]:
                xmin = 565.42
                xmax = 565.45
                text = " al llegar la noche"
            intervals [248]:
                xmin = 566.3
                xmax = 566.59
                text = " el coatí estaba casi resignado con su cautiverio"
            intervals [249]:
                xmin = 568.95
                xmax = 569.0
                text = " Pensaba a cada momento en las cosas ricas que había para comer allí"
            intervals [250]:
                xmin = 570.39
                xmax = 570.78
                text = " y pensaba en aquellos rubios cachorritos de hombre que tan alegres y buenos eran"
            intervals [251]:
                xmin = 572.4
                xmax = 573.13
                text = "          Durante dos noches seguidas"
            intervals [252]:
                xmin = 573.83
                xmax = 574.4
                text = " el perro durmió tan cerca de la jaula"
            intervals [253]:
                xmin = 576.19
                xmax = 576.73
                text = " que la familia del prisionero no se atrevió a acercarse"
            intervals [254]:
                xmin = 577.6
                xmax = 577.63
                text = " con gran sentimiento"
            intervals [255]:
                xmin = 580.17
                xmax = 581.33
                text = " Cuando a la tercera noche llegaron de nuevo a buscar la lima para dar libertad al coaticito"
            intervals [256]:
                xmin = 582.19
                xmax = 583.25
                text = " éste les dijo"
            intervals [257]:
                xmin = 584.36
                xmax = 584.57
                text = "Mamá"
            intervals [258]:
                xmin = 585.72
                xmax = 585.75
                text = " yo no quiero irme más de aquí"
            intervals [259]:
                xmin = 586.64
                xmax = 586.86
                text = " Me dan huevos y son muy buenos conmigo"
            intervals [260]:
                xmin = 587.37
                xmax = 587.82
                text = " Hoy me dijeron que si me portaba bien me iban a dejar suelto muy pronto"
            intervals [261]:
                xmin = 589.32
                xmax = 591.32
                text = " son como nosotros son cachorritos también"
            intervals [262]:
                xmin = 592.43
                xmax = 593.25
                text = " y jugamos juntos"
            intervals [263]:
                xmin = 595.48
                xmax = 597.12
                text = "          Los coatís salvajes quedaron muy tristes"
            intervals [264]:
                xmin = 597.67
                xmax = 598.91
                text = " pero se resignaron"
            intervals [265]:
                xmin = 600.25
                xmax = 600.27
                text = " prometiendo al coaticito venir todas las noches a visitarlo"
            intervals [266]:
                xmin = 601.24
                xmax = 602.22
                text = "          Efectivamente"
            intervals [267]:
                xmin = 606.22
                xmax = 606.26
                text = " todas las noches"
            intervals [268]:
                xmin = 607.51
                xmax = 608.13
                text = " lloviera o no"
            intervals [269]:
                xmin = 611.32
                xmax = 611.34
                text = " su madre y sus hermanos iban a pasar un rato con él"
            intervals [270]:
                xmin = 613.99
                xmax = 614.03
                text = " El coaticito les daba pan por entre el tejido de alambre"
            intervals [271]:
                xmin = 617.98
