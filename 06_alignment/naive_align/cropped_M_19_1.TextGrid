            intervals [355]:
                xmin = 798.38
                xmax = 800.0
                text = " Pero como éste era tan bueno y cariñoso como el otro"
            intervals [356]:
                xmin = 800.64
                xmax = 801.44
                text = " las criaturas no tuvieron la menor sospecha"
            intervals [357]:
                xmin = 802.2
                xmax = 802.51
                text = " Formaron la misma familia de cachorritos de antes"
            intervals [358]:
                xmin = 803.71
                xmax = 805.11
                text = " y"
            intervals [359]:
                xmin = 805.82
                xmax = 806.45
                text = " como antes"
            intervals [360]:
                xmin = 807.07
                xmax = 807.32
                text = " los coatís salvajes venían noche a noche a visitar al coaticito civilizado"
            intervals [361]:
                xmin = 808.38
                xmax = 808.97
                text = " y se sentaban a su lado a comer pedacitos de huevos duros que él les guardaba"
            intervals [362]:
                xmin = 811.9
                xmax = 812.98
                text = " mientras ellos le contaban la vida de la selva"