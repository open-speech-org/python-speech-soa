                text = " y hay otros animales"
            intervals [309]:
                xmin = 699.99
                xmax = 700.01
                text = " como la mangosta que resisten muy bien el veneno de las víboras"
            intervals [310]:
                xmin = 700.89
                xmax = 701.22
                text = " Con toda seguridad el coaticito había sido mordido en una arteria o una vena porque entonces la sangre se envenena en seguida"
            intervals [311]:
                xmin = 705.31
                xmax = 705.32
                text = " y el animal muere"
            intervals [312]:
                xmin = 707.04
                xmax = 707.06
                text = " Esto le había pasado al coaticito"
            intervals [313]:
                xmin = 709.67
                xmax = 710.23
                text = "          Al verlo así"
            intervals [314]:
                xmin = 712.41
                xmax = 712.72
                text = " su madre y sus hermanos lloraron un largo rato"
            intervals [315]:
                xmin = 713.97
                xmax = 714.97
                text = " Después"
            intervals [316]:
                xmin = 716.23
                xmax = 716.27
                text = " como nada más tenían que hacer allí"
            intervals [317]:
                xmin = 719.33
                xmax = 719.79
                text = " salieron de la jaula"
            intervals [318]:
                xmin = 720.62
                xmax = 721.38
                text = " se dieron vuelta para mirar por última vez la casa donde tan feliz había sido el coaticito"
            intervals [319]:
                xmin = 721.91
                xmax = 722.26
                text = " y se fueron otra vez al monte"
            intervals [320]:
                xmin = 723.11
                xmax = 723.38
                text = "          Pero los tres coatís"
            intervals [321]:
                xmin = 727.73
                xmax = 728.87
                text = " sin embargo"
