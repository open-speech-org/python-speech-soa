                text = " Entonces a la madre se le ocurrió de repente una idea"
            intervals [207]:
                xmin = 482.06
                xmax = 482.49
                text = " y dijo"
            intervals [208]:
                xmin = 484.2
                xmax = 484.35
                text = "Vamos a buscar las herramientas del hombre"
            intervals [209]:
                xmin = 489.14
                xmax = 489.62
                text = " Los hombres tienen herramientas para cortar fierro"
            intervals [210]:
                xmin = 490.19
                xmax = 490.67
                text = " Se llaman limas"
            intervals [211]:
                xmin = 491.83
                xmax = 492.08
                text = " Tienen tres lados como las víboras de cascabel"
            intervals [212]:
                xmin = 495.07
                xmax = 495.09
                text = " Se empuja y se retira"
            intervals [213]:
                xmin = 495.84
                xmax = 495.85
                text = "Vamos a buscarla"
            intervals [214]:
                xmin = 498.4
                xmax = 498.42
                text = "          Fueron al taller del hombre y volvieron con la lima"
            intervals [215]:
                xmin = 499.19
                xmax = 499.35
                text = " Creyendo que uno solo no tendría fuerzas bastantes"
            intervals [216]:
                xmin = 500.44
                xmax = 500.48
