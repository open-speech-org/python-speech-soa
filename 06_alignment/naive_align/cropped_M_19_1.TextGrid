            intervals [192]:
                xmin = 455.07
                xmax = 455.12
                text = " mamá"
            intervals [193]:
                xmin = 456.03
                xmax = 456.33
                text = "murmuró el prisionero en voz muy baja para no hacer ruido"
            intervals [194]:
                xmin = 458.22
                xmax = 458.93
                text = "Estoy aquí"
            intervals [195]:
                xmin = 460.33
                xmax = 461.84
                text = "Sáquenme de aquí"
            intervals [196]:
                xmin = 463.3
                xmax = 463.81
                text = "No quiero quedarme"
            intervals [197]:
                xmin = 465.93
                xmax = 467.99
                text = " ma"
            intervals [198]:
                xmin = 469.0
                xmax = 469.53
                text = " má"
            intervals [199]:
                xmin = 470.67
                xmax = 470.72
                text = "y lloraba desconsolado"
            intervals [200]:
                xmin = 471.27
                xmax = 471.56
                text = "          Pero a pesar de todo estaban contentos porque se habían encontrado"
            intervals [201]:
                xmin = 472.51
                xmax = 472.6
                text = " y se hacían mil caricias en el hocico"
            intervals [202]:
                xmin = 473.46
