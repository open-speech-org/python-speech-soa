                xmax = 768.45
                text = " extrañarían un poco algunas cosas"
            intervals [342]:
                xmin = 770.04
                xmax = 770.88
                text = " pero nada más"
            intervals [343]:
                xmin = 777.91
                xmax = 779.7
                text = "          Y así pasó en efecto"
            intervals [344]:
                xmin = 780.33
                xmax = 783.34
                text = " Volvieron a la casa"
            intervals [345]:
                xmin = 785.09
                xmax = 785.43
                text = " y un nuevo coaticito "
            intervals [346]:
                xmin = 786.17
                xmax = 786.25
                text = " reemplazó al primero"
            intervals [347]:
                xmin = 786.92
                xmax = 787.61
                text = " mientras la madre y el otro hermano se llevaban sujetos a los dientes el cadáver del menor"
            intervals [348]:
                xmin = 788.14
                xmax = 788.4
                text = " Lo llevaron despacio al monte"
            intervals [349]:
                xmin = 789.12
                xmax = 789.22
                text = " y la cabeza colgaba"
            intervals [350]:
                xmin = 789.84
                xmax = 789.89
                text = " balanceándose"
            intervals [351]:
                xmin = 790.64
                xmax = 791.33
                text = " y la cola iba arrastrando por el suelo"
            intervals [352]:
                xmin = 792.07
                xmax = 792.45
                text = "          Al día siguiente los chicos extrañaron"
            intervals [353]:
                xmin = 793.13
                xmax = 793.59
                text = " efectivamente"
            intervals [354]:
