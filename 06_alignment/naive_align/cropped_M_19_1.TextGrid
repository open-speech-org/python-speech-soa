                xmin = 729.38
                xmax = 729.39
                text = " iban muy preocupados"
            intervals [323]:
                xmin = 730.06
                xmax = 730.15
                text = " y su preocupación era ésta"
            intervals [324]:
                xmin = 731.62
                xmax = 731.75
                text = "qué iban a decir los chicos"
            intervals [325]:
                xmin = 732.46
                xmax = 732.47
                text = " cuando"
            intervals [326]:
                xmin = 736.67
                xmax = 737.08
                text = " al día siguiente"
            intervals [327]:
                xmin = 737.71
                xmax = 737.73
                text = " vieran muerto a su querido coaticito"
            intervals [328]:
                xmin = 738.43
                xmax = 738.7
                text = " Los chicos le querían muchísimo"
            intervals [329]:
                xmin = 740.66
                xmax = 740.76
                text = " y ellos"
            intervals [330]:
                xmin = 743.29
                xmax = 743.72
                text = " los coatís"
            intervals [331]:
                xmin = 744.46
                xmax = 744.64
                text = " querían también a los cachorritos rubios"
            intervals [332]:
                xmin = 745.57
                xmax = 746.64
                text = " Así es que los tres coatís tenían el mismo pensamiento"
            intervals [333]:
                xmin = 748.12
